@model IEnumerable<StudyEstimator_MVC.Models.Parameter>

@{
    ViewBag.Title = "Current Study Parameters";
}

<h2>Current Study Parameters</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<p>
    This is a test of the session object: 
    @Session["CurrentStudy"] 
</p>
<table>
    <tr>
        <th>
            Param_Item
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Param_Item)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ParameterID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ParameterID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ParameterID })
        </td>
    </tr>
}

</table>
